            <property name="fill_level">1</property>
            <property name="round_digits">2</property>
          </object>
          <packing>
            <property name="expand">True</property>
            <property name="fill">True</property>
            <property name="position">2</property>
          </packing>
        </child>
        <child>
          <object class="GtkHBox" id="hbox1">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="spacing">30</property>
            <property name="homogeneous">True</property>
            <child>
              <object class="HAL_LightButton" id="red_button">
                <property name="width_request">100</property>
                <property name="height_request">100</property>
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="button_text" translatable="yes">red</property>
                <property name="font_off_color">#000000000000</property>
                <property name="font_on_color">#000000000000</property>
                <property name="is_momentary">True</property>
                <property name="light_off_color">#808080808080</property>
                <property name="light_on_color">#ffff00000000</property>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="HAL_LightButton" id="green_button">
                <property name="width_request">100</property>
                <property name="height_request">100</property>
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="button_text" translatable="yes">green</property>
                <property name="font_off_color">#000000000000</property>
                <property name="font_on_color">#000000000000</property>
                <property name="is_momentary">True</property>
                <property name="light_off_color">#808080808080</property>
                <property name="light_on_color">#0000ffff0000</property>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
